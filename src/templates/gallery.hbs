{{#each this}}
<div class="photo-card">
    <div class="photo-wrapper">
        <img src="{{this.webformatURL}}" alt="{{this.tags}}" />
    </div>
    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i>
            &nbsp;{{this.likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i>
            &nbsp;{{this.views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comment</i>
            &nbsp;{{this.comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i>
            &nbsp;{{this.downloads}}
        </p>
    </div>
</div>
{{/each}}